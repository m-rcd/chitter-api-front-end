{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "rules": {
      "react/destructuring-assignment": "never",
      "react/jsx-filename-extension": "never",
      "no-underscore-dangle": 1,
      "react/prop-types": 1,
      "global-require": 0,
      "no-use-before-define": 0,
      "react/sort-comp": 0,
      "react/no-array-index-key": 0,
      "react/prefer-stateless-function": 0
    },
    "env": {
      "jest": true,
      "mocha": true
    },
    "globals": {
      "fetch": true
    }
}
